            }
        ]
    },
    {
        "manipulators": [
            {
                "description": "Change R_Option to FN.",
                "from": {
                    "key_code": "right_option",
                    "modifiers": {
                        "optional": [
                            "any"
                        ]
                    }
                },
                "to": [
                    {
                        "key_code": "fn",
                        "modifiers": []
                    }
                ],
                "type": "basic"
            }
        ]
    },
    {
        "description": "Double tab and hold L_Command to become L_Control.",
        "manipulators": [
            {
                "conditions": [
                    {
                        "name": "left_command pressed",
                        "type": "variable_if",
                        "value": 1
                    }
                ],
