"complex_modifications": {
    "parameters": {
        "basic.simultaneous_threshold_milliseconds": 50,
        "basic.to_delayed_action_delay_milliseconds": 500,
        "basic.to_if_alone_timeout_milliseconds": 500,
        "basic.to_if_held_down_threshold_milliseconds": 50,
        "mouse_motion_to_scroll.speed": 100
    },
    "rules": [
        {
            "description": "Remap CAPS LOCK to Hyper layer when hold.",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "caps_lock",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "hyper_layer_active",
                                "value": 1
                            }
                        },
                        {
                            "key_code": "left_command",
                            "modifiers": [
                                "left_command",
                                "left_control",
                                "left_option",
                                "left_shift"
                            ]
                        }
                    ],
                    "to_after_key_up": [
                        {
                            "set_variable": {
                                "name": "hyper_layer_active",
                                "value": 0
                            }
                        }
                    ]
                }
            ]
        },
        {
            "description": "Remap LEFT SHIFT double press to CAPS LOCK.",
            "manipulators": [
                {
                    "type": "basic",
                    "conditions": [
                        {
                            "name": "caps_lock_active",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "left_shift",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "caps_lock"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "left_shift",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "caps_lock_active",
                                "value": 1
                            }
                        },
                        {
                            "key_code": "left_shift"
                        }
                    ],
                    "to_delayed_action": {
                        "to_if_canceled": [
                            {
                                "set_variable": {
                                    "name": "caps_lock_active",
                                    "value": 0
                                }
                            }
                        ],
                        "to_if_invoked": [
                            {
                                "set_variable": {
                                    "name": "caps_lock_active",
                                    "value": 0
                                }
                            }
                        ]
                    }
                }
            ]
        },
        {
            "description": "Remap RIGHT COMMAND to system layer.",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "right_command",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
