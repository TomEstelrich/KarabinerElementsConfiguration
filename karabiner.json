                            "set_variable": {
                                "name": "system_layer_active",
                                "value": 0
                            }
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "right_command"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Remap RIGHT OPTION to FN.",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "right_option",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "fn",
                            "modifiers": []
                        }
                    ]
                }
            ]
        },
        {
            "description": "Modify quitting applications by pressing COMMAND + Q twice.",
            "manipulators": [
                {
                    "type": "basic",
                    "conditions": [
                        {
                            "name": "command-q_active",
                            "type": "variable_if",
                            "value": 1
                        }
                    ],
                    "from": {
                        "key_code": "q",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ],
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "q",
                            "modifiers": "left_command"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "q",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "command-q_active",
                                "value": 1
                            }
                        }
                    ],
                    "to_delayed_action": {
                        "to_if_canceled": [
                            {
                                "set_variable": {
                                    "name": "command-q_active",
                                    "value": 0
                                }
                            }
                        ],
                        "to_if_invoked": [
                            {
                                "set_variable": {
                                    "name": "command-q_active",
                                    "value": 0
                                }
                            }
                        ]
                    }
                }
            ]
        },
