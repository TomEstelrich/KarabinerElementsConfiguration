                            "set_variable": {
                                "name": "system_layer_active",
                                "value": 1
                            }
                        },
                        {
                            "key_code": "left_command",
                            "modifiers": [
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    ],
                    "to_after_key_up": [
