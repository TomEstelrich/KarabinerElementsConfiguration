                "from": {
                    "key_code": "f",
                    "modifiers": {
                        "mandatory": []
                    }
                },
                "to": [
                    {
                        "shell_command": "open -a 'Finder.app'"
                    }
                ],
                "type": "basic"
            },
            {
                "from": {
                    "modifiers": {
                        "mandatory": []
                    },
                    "simultaneous": [
                        {
                            "key_code": "tab"
                        },
                        {
                            "key_code": "f"
                        }
                    ],
                    "simultaneous_options": {
                        "key_down_order": "strict",
                        "key_up_order": "strict_inverse",
                        "to_after_key_up": [
                            {
                                "set_variable": {
                                    "name": "Tab Launcher",
                                    "value": 0
